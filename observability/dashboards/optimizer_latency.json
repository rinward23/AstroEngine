{
  "title": "Optimizer Latency",
  "schemaVersion": 39,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "Optimizer Duration",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(astroengine_optimizer_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P95"
        },
        {
          "expr": "histogram_quantile(0.5, sum(rate(astroengine_optimizer_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P50"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    }
  ]
}
